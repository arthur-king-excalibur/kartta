/bin/sh /container/tools/subst \
  /container/secrets/secrets.env \
  /container/config/editor/database.yml.in       /srv/editor-website/config/database.yml \
  /container/config/editor/storage.yml.in        /srv/editor-website/config/storage.yml \
  /container/config/editor/settings.local.yml.in /srv/editor-website/config/settings.local.yml \
  /container/config/editor/nginx.conf.in         /etc/nginx/sites-enabled/editor
