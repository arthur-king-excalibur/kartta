/bin/rm -f /etc/nginx/sites-enabled/default

/bin/bash /container/tools/subst \
  /container/secrets/secrets.env \
  /container/config/mapwarper/application.yml.in          /app/config/application.yml \
  /container/config/mapwarper/database.yml.in             /app/config/database.yml \
  /container/config/mapwarper/nginx.conf.in               /etc/nginx/sites-enabled/mapwarper \
  /container/config/mapwarper/mapwarper-rails-start.sh.in /mapwarper-rails-start.sh
  

. /container/secrets/secrets.env

echo ${MAPWARPER_SA_KEY_JSON} > /mapwarper-sa-key.json
